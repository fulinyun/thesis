@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc11: <http://purl.org/dc/elements/1.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/> .

<http://purl.org/net/nknouf/ns/bibtex>
  a owl:Ontology ;
  dc11:creator "Nick Knouf <nknouf@mit.edu>" ;
  dc11:description "Transformation of bibTeX into an OWL ontology" ;
  dc11:date "2003-12-29" ;
  dc11:subject "fMRI, ontology, fMRI analysis" ;
  rdfs:label "bibtex ontology" ;
  dc11:title "bibtex.owl: an ontology for bibTeX entries" ;
  dc11:identifier "http://zeitkunst.org/bibtex/0.2/bibtex.owl"^^xsd:anyURI ;
  rdfs:comment "Possible ontology to describe bibTeX entries." ;
  owl:versionInfo "0.2" .

dc11:creator
  a owl:AnnotationProperty ;
  dc11:title "Nicholas Knouf" .

dc11:description a owl:AnnotationProperty .
dc11:date a owl:AnnotationProperty .
dc11:subject a owl:AnnotationProperty .
dc11:title a owl:AnnotationProperty .
dc11:source a owl:AnnotationProperty .
dc11:identifier a owl:AnnotationProperty .
dc:abstract a owl:AnnotationProperty .
dc:bibliographicCitation a owl:AnnotationProperty .
<http://purl.org/net/nknouf/ns/bibtex#Entry>
  a owl:Class ;
  rdfs:label "Entry"@en ;
  rdfs:comment "Base class for all entries"@en .

<http://purl.org/net/nknouf/ns/bibtex#Article>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasAuthor> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasJournal> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Article"@en ;
  rdfs:comment "An article from a journal or magazine."@en .

<http://purl.org/net/nknouf/ns/bibtex#Book>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#humanCreator> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger ;
    dc11:description "One thing to figure out is whether or not this tells us what we want; that is, does this imply that we must have at least one property of humanCreator, which could be either hasAuthor or hasEditor?"
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasPublisher> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Book"@en ;
  rdfs:comment "A book with an explicit publisher."@en .

<http://purl.org/net/nknouf/ns/bibtex#Booklet>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Booklet"@en ;
  rdfs:comment "A work that is printed and bound, but without a named publisher or sponsoring institution."@en .

<http://purl.org/net/nknouf/ns/bibtex#Conference>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  owl:equivalentClass <http://purl.org/net/nknouf/ns/bibtex#Inproceedings> ;
  rdfs:label "Conference"@en ;
  rdfs:comment "The same as INPROCEEDINGS, included for Scribe compatibility."@en .

<http://purl.org/net/nknouf/ns/bibtex#Inbook>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#humanCreator> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger ;
    dc11:description "One thing to figure out is whether or not this tells us what we want; that is, does this imply that we must have at least one property of humanCreator, which could be either hasAuthor or hasEditor?"
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#pageChapterData> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasPublisher> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Inbook"@en ;
  rdfs:comment "A part of a book, which may be a chapter (or section or whatever) and/or a range of pages. "@en .

<http://purl.org/net/nknouf/ns/bibtex#Incollection>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasAuthor> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasBooktitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasPublisher> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Incollection"@en ;
  rdfs:comment "A part of a book having its own title."@en .

<http://purl.org/net/nknouf/ns/bibtex#Inproceedings>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasAuthor> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasBooktitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Inproceedings"@en ;
  rdfs:comment "An article in a conference proceedings."@en .

<http://purl.org/net/nknouf/ns/bibtex#Manual>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Manual"@en ;
  rdfs:comment "Technical documentation."@en .

<http://purl.org/net/nknouf/ns/bibtex#Mastersthesis>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasAuthor> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasSchool> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Mastersthesis"@en ;
  rdfs:comment "A Master's thesis."@en .

<http://purl.org/net/nknouf/ns/bibtex#Misc>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:label "Misc"@en ;
  rdfs:comment "Use this type when nothing else fits."@en .

<http://purl.org/net/nknouf/ns/bibtex#Phdthesis>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasAuthor> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasSchool> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Phdthesis"@en ;
  rdfs:comment "A PhD thesis."@en .

<http://purl.org/net/nknouf/ns/bibtex#Proceedings>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Proceedings"@en ;
  rdfs:comment "The proceedings of a conference."@en .

<http://purl.org/net/nknouf/ns/bibtex#Techreport>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasAuthor> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasInstitution> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasYear> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Techreport"@en ;
  rdfs:comment "A report published by a school or other institution, usually numbered within a series."@en .

<http://purl.org/net/nknouf/ns/bibtex#Unpublished>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/net/nknouf/ns/bibtex#Entry>, [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasAuthor> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasTitle> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ], [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/net/nknouf/ns/bibtex#hasNote> ;
    owl:minCardinality "1"^^xsd:nonNegativeInteger
  ] ;
  rdfs:label "Unpublished"@en ;
  rdfs:comment "A document having an author and title, but not formally published."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasKey>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has key"@en ;
  rdfs:comment "The key for a particular bibTeX entry.  Note that the rdf:ID for each Entry instance could be the bibTeX key as well, possibly making this property redundant."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasAddress>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has address"@en ;
  rdfs:comment "Usually the address of the publisher or other type of institution. For major publishing houses, van Leunen recommends omitting the information entirely. For small publishers, on the other hand, you can help the reader by giving the complete address."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasAnnotation>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has annotation"@en ;
  rdfs:comment "An annotation. It is not used by the standard bibliography styles, but may be used by others that produce an annotated bibliography."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasAuthor>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf <http://purl.org/net/nknouf/ns/bibtex#humanCreator> ;
  rdfs:label "has author"@en ;
  rdfs:comment "The name(s) of the author(s), in the format described in the LaTeX book."@en ;
  dc11:description "This is tricky due to the fact that order is not (generally) preserved in RDF documents.  The problem arises when you want to have an author list where the order is _extremely_ important.  How shall we do that?  Perhaps we want to define \"hasPrimaryAuthor\", \"hasSecondaryAuthor\", \"hasTertiaryAuthor\", and \"hasRemainingAuthors\", or something of that sort.  This will be have to given more thought." .

<http://purl.org/net/nknouf/ns/bibtex#hasBooktitle>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has booktitle"@en ;
  rdfs:comment "Title of a book, part of which is being cited. See the LaTeX book for how to type titles. For book entries, use the title field instead."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasChapter>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:nonNegativeInteger ;
  rdfs:subPropertyOf <http://purl.org/net/nknouf/ns/bibtex#pageChapterData> ;
  rdfs:label "has chapter"@en ;
  rdfs:comment "A chapter (or section or whatever) number."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasCrossref>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has crossref"@en ;
  rdfs:comment "The database key of the entry being cross referenced."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasEdition>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has edition"@en ;
  rdfs:comment "The edition of a book--for example, \"Second\". This should be an ordinal, and should have the first letter capitalized, as shown here; the standard styles convert to lower case when necessary."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasEditor>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf <http://purl.org/net/nknouf/ns/bibtex#humanCreator> ;
  rdfs:label "has editor"@en ;
  rdfs:comment "Name(s) of editor(s), typed as indicated in the LaTeX book. If there is also an author field, then the editor field gives the editor of the book or collection in which the reference appears."@en ;
  dc11:description "Again, the same issues that arose with the \"hasAuthor\" property apply here." .

<http://purl.org/net/nknouf/ns/bibtex#howPublished>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "how published"@en ;
  rdfs:comment "How something strange has been published. The first word should be capitalized."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasInstitution>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has institution"@en ;
  rdfs:comment "The sponsoring institution of a technical report."@en ;
  dc11:description "This could be an object property that refers to an external set of institution instances." .

<http://purl.org/net/nknouf/ns/bibtex#hasJournal>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has journal"@en ;
  rdfs:comment "A journal name. Abbreviations are provided for many journals; see the Local Guide"@en ;
  dc11:description "This could optionally be an object property, whereby the range would refer to an external set of journal instances, thus providing standardized abbreviations for different bibliographic styles." .

<http://purl.org/net/nknouf/ns/bibtex#hasMonth>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has month"@en ;
  rdfs:comment "The month in which the work was published or, for an unpublished work, in which it was written. You should use the standard three-letter abbreviation, as described in Appendix B.1.3 of the LaTeX book."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasNote>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has note"@en ;
  rdfs:comment "Any additional information that can help the reader. The first word should be capitalized."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasNumber>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has number"@en ;
  rdfs:comment "The number of a journal, magazine, technical report, or of a work in a series. An issue of a journal or magazine is usually identified by its volume and number; the organization that issues a technical report usually gives it a number; and sometimes books are given numbers in a named series."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasOrganization>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has organization"@en ;
  rdfs:comment "The organization that sponsors a conference or that publishes a manual."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasPages>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf <http://purl.org/net/nknouf/ns/bibtex#pageChapterData> ;
  rdfs:label "has pages"@en ;
  rdfs:comment "One or more page numbers or range of numbers, such as 42-111 or 7,41,73-97 or 43+ (the `+' in this last example indicates pages following that don't form a simple range). To make it easier to maintain Scribe-compatible databases, the standard styles convert a single dash (as in 7-33) to the double dash used in TeX to denote number ranges (as in 7-33)."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasPublisher>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has publisher"@en ;
  rdfs:comment "The publisher's name."@en ;
  dc11:description "This is a case where an ObjectProperty might be a better choice, where the range is some set of publisher names defined in another ontology.  That would allow all of the metadata for the publisher to be incorporated as needed." .

<http://purl.org/net/nknouf/ns/bibtex#hasSchool>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has school"@en ;
  rdfs:comment "The name of the school where a thesis was written."@en ;
  dc11:description "As with \"hasPublisher\", this could be an ObjectProperty that refers to an external set of school instances." .

<http://purl.org/net/nknouf/ns/bibtex#hasSeries>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has series"@en ;
  rdfs:comment "The name of a series or set of books. When citing an entire book, the the title field gives its title and an optional series field gives the name of a series or multi-volume set in which the book is published."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasTitle>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has title"@en ;
  rdfs:comment "The work's title, typed as explained in the LaTeX book."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasType>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:string ;
  rdfs:label "has type"@en ;
  rdfs:comment "The type of a technical report--for example, \"Research Note\"."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasVolume>
  a owl:DatatypeProperty ;
  rdfs:domain <http://purl.org/net/nknouf/ns/bibtex#Entry> ;
  rdfs:range xsd:nonNegativeInteger ;
  rdfs:label "has volume"@en ;
  rdfs:comment "The volume of a journal or multivolume book."@en .

<http://purl.org/net/nknouf/ns/bibtex#hasYear>
